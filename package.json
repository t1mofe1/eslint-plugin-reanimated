{
  "name": "@t1m0fe1/eslint-plugin-reanimated",
  "version": "0.0.1",
  "description": "ESLint plugin for Reanimated",
  "main": "dist/index.js",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "reanimated",
    "react-native-reanimated",
    "eslint-reanimated",
    "eslint-plugin-reanimated"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/t1m0fe1/eslint-plugin-reanimated.git"
  },
  "bugs": {
    "url": "https://github.com/t1m0fe1/eslint-plugin-reanimated/issues"
  },
  "homepage": "https://github.com/t1m0fe1/eslint-plugin-reanimated#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "lint": "bunx eslint .",
    "build": "bunx tsc -p tsconfig.build.json",
    "prepublish": "bun run build",
    "update:eslint-docs": "bun run build && eslint-doc-generator"
  },
  "peerDependencies": {
    "@types/eslint": ">=9.0.0",
    "eslint": ">=9.0.0",
    "typescript": ">=5.8.2"
  },
  "peerDependenciesMeta": {
    "@types/eslint": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/bun": "^1.2.5",
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@typescript-eslint/rule-tester": "^8.26.1",
    "@typescript-eslint/utils": "^8.26.1",
    "eslint": "^8.57.1",
    "eslint-doc-generator": "^2.1.1",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-plugin-n": "^17.16.2",
    "semantic-release": "^24.2.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
